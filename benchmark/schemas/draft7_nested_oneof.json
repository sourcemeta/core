{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "oneOf": [
    {
      "description": "First branch with nested oneOf",
      "oneOf": [
        {
          "description": "Option 1A",
          "type": "object",
          "properties": {
            "fieldA1": { "type": "string" }
          },
          "required": ["fieldA1"]
        },
        {
          "description": "Option 1B",
          "type": "object",
          "properties": {
            "fieldA2": { "type": "number" }
          },
          "required": ["fieldA2"]
        }
      ]
    },
    {
      "description": "Second branch with nested oneOf",
      "oneOf": [
        {
          "description": "Option 2A",
          "type": "object",
          "properties": {
            "fieldB1": { "type": "boolean" }
          },
          "required": ["fieldB1"]
        },
        {
          "description": "Option 2B",
          "type": "object",
          "properties": {
            "fieldB2": { "type": "null" }
          },
          "required": ["fieldB2"]
        },
        {
          "description": "Option 2C",
          "type": "object",
          "properties": {
            "fieldB3": { "type": "array", "items": { "type": "string" } }
          },
          "required": ["fieldB3"]
        }
      ]
    },
    {
      "description": "Third branch with nested oneOf",
      "oneOf": [
        {
          "description": "Option 3A",
          "type": "object",
          "properties": {
            "fieldC1": { "type": "string" }
          },
          "required": ["fieldC1"]
        },
        {
          "description": "Option 3B",
          "type": "object",
          "properties": {
            "fieldC2": { "type": "integer" }
          },
          "required": ["fieldC2"]
        }
      ]
    },
    {
      "description": "Fourth branch with nested oneOf",
      "oneOf": [
        {
          "description": "Option 4A",
          "type": "object",
          "properties": {
            "fieldD1": { "type": "boolean" }
          },
          "required": ["fieldD1"]
        },
        {
          "description": "Option 4B",
          "type": "object",
          "properties": {
            "fieldD2": { "type": "object", "properties": { "nestedField": { "type": "number" } }, "required": ["nestedField"] }
          },
          "required": ["fieldD2"]
        }
      ]
    },
    {
      "description": "Fifth branch with nested oneOf",
      "oneOf": [
        {
          "description": "Option 5A",
          "type": "object",
          "properties": {
            "fieldE1": { "type": "null" }
          },
          "required": ["fieldE1"]
        },
        {
          "description": "Option 5B",
          "type": "object",
          "properties": {
            "fieldE2": { "type": "string" }
          },
          "required": ["fieldE2"]
        }
      ]
    }
  ]
}
